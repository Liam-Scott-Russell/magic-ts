{
  "name": "magic-ts",
  "version": "0.0.0",
  "description": "Typescript types so good they're magic!",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest",
    "lint": "prettier --check . && eslint --ext .ts .",
    "lint:fix": "prettier --write '**/*.{ts, md, json}' && eslint --fix --ext .ts .",
    "build": "rm -rf ./dist && npm run barrel && tsc -p tsconfig.build.json && echo 'Built magic-ts!'",
    "check-deps": "madge --circular --warning --extensions ts --ts-config ./tsconfig.build.json .",
    "barrel": "barrelsby --delete --directory=./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liam-Scott-Russell/magic-ts.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Liam-Scott-Russell/magic-ts/issues"
  },
  "homepage": "https://github.com/Liam-Scott-Russell/magic-ts#readme",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "barrelsby": "^2.3.4",
    "eslint": "^8.16.0",
    "eslint-config-canonical": "^35.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^28.1.0",
    "madge": "^5.0.1",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  }
}
